{
    "$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
    "version": "0.2",
    "language": "en-GB",
    "languageId": "python",
    "useGitignore": true,
    "dictionaries": [
        "!aws",
        "!companies",
        "!cryptocurrencies",
        "!web-services"
    ],
    "cache": {
        "cacheFormat": "universal",
        "cacheLocation": ".cache/.cspellcache",
        "cacheStrategy": "content",
        "useCache": true
    },
    "ignorePaths": [
        "cspell.json",
        "fortran_lib.f90",
        "meta.yaml",
        "pyproject.toml",
        "activex.py"
    ],
    "words": [
        "loadlib",
        "labview",
        "roszman",
        "arctan"
    ]
}
